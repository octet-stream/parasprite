{
  "name": "parasprite",
  "version": "1.0.0-beta.7",
  "description": "Describe your GraphQL schema using chainable interface",
  "main": "./parasprite.js",
  "author": "Nick K.",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": "octet-stream/parasprite",
  "keywords": [
    "graphql",
    "wrapper",
    "chains",
    "schema"
  ],
  "scripts": {
    "make": "node_modules/.bin/babel src -d .",
    "m": "yarn make",
    "watch": "node_modules/.bin/babel src -w -d .",
    "w": "yarn watch",
    "make:spec": "node_modules/.bin/babel src -s -d .",
    "test": "node_modules/.bin/ava",
    "t": "yarn test",
    "test:watch": "node_modules/.bin/ava --watch",
    "t:w": "yarn test:watch",
    "cleanup": "node_modules/.bin/rimraf helper lib test .map ..map *.map Input.js Interface.js Schema.js Type.js Enum.js introspect.js parasprite.js toListType.js toRequired.js buildSchema.js",
    "coverage": "node_modules/.bin/nyc yarn test",
    "lint": "node_modules/.bin/eslint src",
    "report": "yarn make:spec && yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test && yarn cleanup && yarn make",
    "report:codecov": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "codecov": "node_modules/.bin/codecov",
    "ci": "yarn make:spec && yarn coverage && yarn report:codecov && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn report"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "test/**/*.js",
      "!test/**/__hook__/**",
      "!test/**/__helper__/**",
      "!test/helper/**"
    ]
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.51",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.51",
    "@babel/plugin-transform-async-to-generator": "7.0.0-beta.51",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.51",
    "@octetstream/eslint-config": "2.1.0",
    "ava": "0.25.0",
    "babel-eslint": "8.2.5",
    "codecov": "3.0.2",
    "eslint": "5.0.1",
    "eslint-plugin-ava": "4.5.1",
    "graphql": "0.13.2",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "nyc": "12.0.2",
    "proxyquire": "2.0.1",
    "rimraf": "2.6.2",
    "sinon": "6.0.1"
  },
  "peerDependencies": {
    "graphql": ">=0.11.x || < 1.x"
  },
  "dependencies": {
    "@octetstream/invariant": "1.1.0",
    "@octetstream/promisify": "1.3.3",
    "lodash.isempty": "4.4.0",
    "lodash.merge": "4.6.1",
    "lodash.omitby": "4.6.0"
  }
}
