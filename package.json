{
  "name": "parasprite",
  "version": "0.3.2",
  "description": "Describe your GraphQL schema using chainable interface",
  "main": "./lib/parasprite.js",
  "author": "Nick K.",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octet-stream/parasprite.git"
  },
  "bugs": {
    "url": "https://github.com/octet-stream/parasprite/issues"
  },
  "keywords": [
    "graphql",
    "wrapper",
    "chains",
    "schema"
  ],
  "scripts": {
    "lint": "node_modules/.bin/eslint bin && node_modules/.bin/eslint src && node_modules/.bin/eslint test",
    "make": "bin/cake make",
    "m": "npm run make",
    "watch": "bin/cake watch",
    "w": "npm run watch",
    "test": "node_modules/.bin/ava",
    "t": "npm run test",
    "test:watch": "node_modules/.bin/ava --watch",
    "t:w": "npm run test:watch",
    "coverage": "npm run make && node_modules/.bin/nyc npm run test",
    "report": "npm run coverage && node_modules/.bin/nyc report --reporter=html npm run test",
    "report:codecov": "node_modules/.bin/nyc report --reporter=lcov npm run test",
    "codecov": "node_modules/.bin/codecov",
    "ci": "npm run coverage && npm run report:codecov && npm run codecov"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit",
    "files": [
      "./test/unit/**/*.js",
      "!./test/helper/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "0.18.2",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-class-properties": "6.23.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.23.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-register": "6.23.0",
    "chalk": "1.1.3",
    "chokidar": "1.6.1",
    "codecov": "1.0.1",
    "commander": "2.9.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "figures": "2.0.0",
    "glob": "7.1.1",
    "graphql": "0.9.x",
    "gulp-sourcemaps": "2.4.1",
    "gulp-util": "3.0.8",
    "junk": "2.0.0",
    "nyc": "10.1.2",
    "ora": "1.1.0",
    "rimraf": "2.6.1",
    "sinon": "1.17.7",
    "through2": "2.0.3",
    "vinyl-fs": "2.4.4",
    "vinyl-sourcemaps-apply": "0.2.1"
  },
  "peerDependencies": {
    "graphql": "0.9.x"
  },
  "dependencies": {
    "lodash.isplainobject": "4.0.6"
  }
}
