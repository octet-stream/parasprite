{
  "name": "parasprite",
  "version": "0.4.2",
  "description": "Describe your GraphQL schema using chainable interface",
  "main": "./parasprite.js",
  "author": "Nick K.",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octet-stream/parasprite.git"
  },
  "bugs": {
    "url": "https://github.com/octet-stream/parasprite/issues"
  },
  "keywords": [
    "graphql",
    "wrapper",
    "chains",
    "schema"
  ],
  "scripts": {
    "make": "node_modules/.bin/babel src -d .",
    "m": "yarn make",
    "watch": "node_modules/.bin/babel src -w -d .",
    "w": "yarn watch",
    "make:spec": "node_modules/.bin/babel src -s -d .",
    "test": "node_modules/.bin/ava",
    "t": "yarn test",
    "test:watch": "node_modules/.bin/ava --watch",
    "t:w": "yarn test:watch",
    "coverage": "node_modules/.bin/nyc yarn test",
    "lint": "node_modules/.bin/eslint src",
    "report": "yarn make:spec && yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test",
    "report:codecov": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "codecov": "node_modules/.bin/codecov",
    "ci": "yarn make:spec && yarn coverage && yarn report:codecov && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn report"
  },
  "ava": {
    "files": [
      "./test/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "codecov": "2.2.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "graphql": "0.10.x",
    "husky": "0.13.3",
    "nyc": "11.0.1",
    "proxyquire": "1.8.0",
    "rimraf": "2.6.1",
    "sinon": "2.3.2"
  },
  "peerDependencies": {
    "graphql": "0.10.x"
  },
  "dependencies": {
    "lodash.isplainobject": "4.0.6"
  }
}
