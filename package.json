{
  "name": "parasprite",
  "version": "1.0.0-beta.3",
  "description": "Describe your GraphQL schema using chainable interface",
  "main": "./parasprite.js",
  "author": "Nick K.",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": "octet-stream/parasprite",
  "keywords": [
    "graphql",
    "wrapper",
    "chains",
    "schema"
  ],
  "scripts": {
    "make": "node_modules/.bin/babel src -d .",
    "m": "yarn make",
    "watch": "node_modules/.bin/babel src -w -d .",
    "w": "yarn watch",
    "make:spec": "node_modules/.bin/babel src -s -d .",
    "test": "node_modules/.bin/ava",
    "t": "yarn test",
    "test:watch": "node_modules/.bin/ava --watch",
    "t:w": "yarn test:watch",
    "cleanup": "node_modules/.bin/rimraf helper lib test .map ..map *.map Input.js Interface.js Schema.js Type.js Enum.js introspect.js parasprite.js toListType.js toRequired.js buildSchema.js",
    "coverage": "node_modules/.bin/nyc yarn test",
    "lint": "node_modules/.bin/eslint src",
    "report": "yarn make:spec && yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test && yarn cleanup && yarn make",
    "report:codecov": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "codecov": "node_modules/.bin/codecov",
    "ci": "yarn make:spec && yarn coverage && yarn report:codecov && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn report"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "test/**/*.js",
      "!test/**/__hook__/**",
      "!test/**/__helper__/**",
      "!test/helper/**"
    ]
  },
  "devDependencies": {
    "@octetstream/eslint-config": "2.1.0",
    "ava": "0.23.0",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "codecov": "3.0.0",
    "eslint": "4.10.0",
    "eslint-plugin-ava": "4.2.2",
    "graphql": ">=0.11.x",
    "husky": "0.14.3",
    "nyc": "11.2.1",
    "proxyquire": "1.8.0",
    "rimraf": "2.6.2",
    "sinon": "4.0.2"
  },
  "peerDependencies": {
    "graphql": ">=0.11.x"
  },
  "dependencies": {
    "@octetstream/invariant": "1.1.0",
    "@octetstream/promisify": "1.3.2",
    "lodash.isempty": "4.4.0",
    "lodash.merge": "4.6.0",
    "lodash.omitby": "4.6.0"
  }
}
