{
  "name": "parasprite",
  "version": "0.3.5",
  "description": "Describe your GraphQL schema using chainable interface",
  "main": "./parasprite.js",
  "author": "Nick K.",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octet-stream/parasprite.git"
  },
  "bugs": {
    "url": "https://github.com/octet-stream/parasprite/issues"
  },
  "keywords": [
    "graphql",
    "wrapper",
    "chains",
    "schema"
  ],
  "scripts": {
    "make": "bin/cake make",
    "m": "yarn make",
    "watch": "bin/cake watch",
    "w": "yarn watch",
    "test": "node_modules/.bin/ava",
    "t": "yarn test",
    "test:watch": "node_modules/.bin/ava --watch",
    "t:w": "yarn test:watch",
    "coverage": "node_modules/.bin/nyc yarn test",
    "lint": "node_modules/.bin/eslint bin && node_modules/.bin/eslint bin src test",
    "report": "yarn make && yarn coverage && node_modules/.bin/nyc report --reporter=html yarn test",
    "report:codecov": "node_modules/.bin/nyc report --reporter=lcov yarn test",
    "codecov": "node_modules/.bin/codecov",
    "ci": "yarn make && yarn coverage && yarn report:codecov && yarn codecov",
    "precommit": "yarn lint",
    "prepush": "yarn report"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit",
    "files": [
      "./test/**/*.js"
    ]
  },
  "devDependencies": {
    "ava": "0.19.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-register": "6.23.0",
    "chalk": "1.1.3",
    "chokidar": "1.7.0",
    "codecov": "2.2.0",
    "commander": "2.9.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-standard-react": "4.2.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "figures": "2.0.0",
    "glob": "7.1.2",
    "graphql": "0.10.x",
    "gulp-sourcemaps": "2.6.0",
    "gulp-util": "3.0.8",
    "husky": "0.13.3",
    "junk": "2.0.0",
    "nyc": "10.3.2",
    "ora": "1.2.0",
    "rimraf": "2.6.1",
    "sinon": "2.3.2",
    "through2": "2.0.3",
    "vinyl-fs": "2.4.4",
    "vinyl-sourcemaps-apply": "0.2.1"
  },
  "peerDependencies": {
    "graphql": "0.10.x"
  },
  "dependencies": {
    "lodash.isplainobject": "4.0.6"
  }
}
